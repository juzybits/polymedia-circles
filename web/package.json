{
    "private": true,
    "version": "0.0.0",
    "name": "@polymedia/circles-web",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://circles.polymedia.app",
    "description": "Generative art on Sui",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-circles"
    },
    "keywords": [],
    "scripts": {
        "clean": "rm -rf .parcel-cache/ dist/",
        "lint": "eslint ./src --ext .ts,.tsx",
        "serve": "pnpm clean && parcel src/index.html",
        "build": "pnpm clean && parcel build --no-source-maps src/index.html && cp src/static/* dist/",
        "deploy-dev": "pnpm build && wrangler pages publish dist/ --project-name polymedia-circles --branch dev",
        "deploy-prod": "pnpm build && wrangler pages publish dist/ --project-name polymedia-circles --branch main",
        "generate-sdk": "./sui-client-gen/generate-sdk.sh"
    },
    "dependencies": {
        "@mysten/bcs": "latest",
        "@mysten/sui.js": "latest",
        "@mysten/wallet-kit": "latest",
        "@mysten/wallet-kit-core": "latest",
        "@polymedia/react-components": "latest",
        "@polymedia/webutils": "^2.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.3"
    },
    "devDependencies": {
        "@parcel/transformer-less": "latest",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "eslint-plugin-deprecation": "latest",
        "events": "latest",
        "parcel": "latest",
        "prettier": "latest",
        "process": "latest",
        "react-devtools": "latest"
    }
}
